local library = loadstring(game:HttpGet(('https://raw.githubusercontent.com/AikaV3rm/UiLib/master/Lib.lua')))()

local w = library:CreateWindow("Vl_qs BP GUI") -- Creates the window

local b = w:CreateFolder("Main") -- Creates the folder(U will put here your buttons,etc

local c = w:CreateFolder("Misc")



c:Button("Inf Yield",function()
    loadstring(game:HttpGet('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'))()
end)

c:DestroyGui()

b:Button("Credit Script",function()
  loadstring(game:HttpGet('https://pastebin.com/raw/VTAc9qau'))()
end)

b:Button("BP GUI (1iseo)",function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/1iseo/breaking-point-public/main/main.lua"))()
end)
